view, scroll-view, swiper, swiper-item, movable-area, movable-view, cover-view,
cover-image, icon, text, rich-text, progress, button, checkbox-group, checkbox,
form, input, label, picker, picker-view, radio-group, radio, slider, switch,
textarea, navigator, functional-page-navigator, image, video, camera,
live-player, live-pusher, map, canvas, open-data, web-view, ad {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  will-change: transform;
}

button {
  background: none;
  border: 0;
  line-height: unset;
  margin: 0;
  padding: 0;
}

.button-hover {
  background: none;
  color: #222;
}

button::before {
  border: 0;
}

button::after {
  border: 0;
}

.index_box {
  padding: 51rpx 24rpx;
}

.index_rec {
  margin: 0 0 31rpx 0;
  font-size: 30rpx;
  color: #222;
  position: relative;
}

.index_rec .rec_text {
  position: absolute;
  left: 0;
  bottom: 5rpx;
  width: 57rpx;
  height: 22rpx;
  z-index: -1;
  background: rgba(244, 214, 84, 0.65);
}

.index_list {
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 24rpx;
}

.index_item {
  width: 46%;
  margin: 0 0 31rpx 0;
}

.index_item_img {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  border-radius: 5px;
  height: 341rpx;
  margin: 0 0 31rpx 0;
  overflow: hidden;
}

.index_item image {
  width: 100%;
}

.index_name {
  color: #222;
  font-size: 28rpx;
}

.index_des {
  color: #666;
  font-size: 24rpx;
  margin: 19rpx 0;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.pre_p {
  font-size: 26rpx;
  color: #f80f0f;
}

.old_p {
  font-size: 26rpx;
  color: #999;
  margin: 0 0 0 21rpx;
  text-decoration: line-through;
}

.d_nav {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 2;
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* padding: 0 67rpx; */
  font-size: 28rpx;
  color: #222;
  border-bottom: 1px solid #fddb61;
  background-color: #fff;
}

.d_nav .view {
  flex: 1;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
}

.d_active {
  color: #fdce02;
  font-size: 30rpx;
  transition: all 0.5s;
}

page {
  background-color: #f9f9fa;
}

.form_item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 25rpx;
  font-size: 28rpx;
  background-color: #fff;
  border-bottom: 1px solid #d2d2d2;
}

.form_input {
  text-align: right;
  flex: 1;
  position: relative;
}

.form_input input {
  padding: 27rpx 0;
  width: 100%;
  height: auto;
}

.form_input .placle {
  color: #999;
  font-size: 26rpx;
}

.picker {
  padding: 0 0 0 25rpx;
}

.form_input picker {
  /* position: absolute; *//* width: 100%; *//* height: 100%; *//* left: 0; *//* right: 0; *//* z-index: 2; */
  flex: 1;
  font-size: 26rpx;
  text-align: right;
  color: #999;
  padding: 27rpx 0;
  /* display: flex;
   align-items: center;
   justify-content: flex-end; */
  overflow: hidden;
}

.form_input .iconfont {
  color: #e4e4e4;
  font-size: 26rpx;
  margin: 5rpx 15rpx 0 15rpx;
  /* margin-left: 20rpx; */
}

.form_submit {
  width: 37%;
  background-color: #fdd808;
  color: #222;
  font-size: 34rpx;
  text-align: center;
  padding: 20rpx 0;
  border-radius: 10rpx;
  margin: 200rpx auto 0 auto;
}

.form_submit:active {
  opacity: 0.7;
}

.nodata {
  padding: 25px 0;
  text-align: center;
  font-size: 25rpx;
  color: #cccc;
}
